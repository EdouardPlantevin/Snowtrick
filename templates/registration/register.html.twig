{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
{% endblock %}

{% block body %}
    <section class="h-100 gradient-form" id="login">
        <div class="container py-5 h-100">
            <div class="row d-flex justify-content-center align-items-center h-100">
                <div class="col-xl-10">
                    <div class="card rounded-3 text-black">
                        <div class="row g-0">
                            <div class="col-lg-6">
                                <div class="card-body p-md-5 mx-md-4">

                                    <div class="text-center">
                                        <img src="data:image/png;base64," style="width: 185px;" alt="logo">
                                    </div>

                                    {{ form_start(registrationForm) }}

                                    <div class="form-outline mb-4">
                                        <label for="inputUsername">Email</label>
                                        {{ form_row(registrationForm.email) }}
                                    </div>

                                    <div class="form-outline mb-4">
                                        <label for="inputUsername">Pseudo</label>
                                        {{ form_row(registrationForm.username) }}
                                    </div>

                                    <div class="form-outline mb-4">
                                        <label for="inputUsername">Mot de passe</label>
                                        {{ form_row(registrationForm.plainPassword) }}
                                    </div>

                                    <div class="form-outline mb-4">
                                        <label for="inputUsername">Accepter les conditions</label>
                                        {{ form_row(registrationForm.agreeTerms) }}
                                    </div>

                                    <div class="text-center pt-1 mb-5 pb-1">
                                        <button class="btn btn-primary btn-block fa-lg gradient-custom-2 mb-3 w-100" type="submit">Inscription</button>
                                        <a class="text-muted" href="{{ path('app_login') }}">Déjà un compte ?</a>
                                    </div>

                                    {{ form_end(registrationForm) }}

                                </div>
                            </div>
                            <div class="col-lg-6 d-flex align-items-center gradient-custom-2">
                                <div class="text-white px-3 py-4 p-md-5 mx-md-4">
                                    <h4 class="mb-4">Nous sommes plus qu'une simple entreprise</h4>
                                    <p class="small mb-0">
                                        Nous sommes une communauté dont vous faites partie, nous avons besoin de vous de votre expérience et de votre sens du partage pour alimenter le site internet<br>
                                        Vous allez être les acteurs principaux de ce site. Prêt ? Action !
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock %}

